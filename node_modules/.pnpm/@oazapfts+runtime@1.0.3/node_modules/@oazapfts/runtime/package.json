{
  "name": "@oazapfts/runtime",
  "version": "1.0.3",
  "description": "Runtime for OpenApi TypeScript client generator",
  "type": "module",
  "main": "index.cjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./query": {
      "types": "./query.d.ts",
      "require": "./query.cjs",
      "default": "./query.js"
    },
    "./util": {
      "types": "./util.d.ts",
      "require": "./util.cjs",
      "default": "./util.js"
    },
    "./headers": {
      "types": "./headers.d.ts",
      "require": "./headers.cjs",
      "default": "./headers.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oazapfts/oazapfts",
    "directory": "packages/runtime"
  },
  "scripts": {
    "build": "rm -rf dist && vite build --mode esm && vite build --mode cjs",
    "prepack": "sed -i 's|||g' package.json && mv * ."
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "keywords": [
    "openapi",
    "typescript",
    "runtime",
    "http",
    "oazapfts"
  ]
}
